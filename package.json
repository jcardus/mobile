    "@turf/bearing": "latest",
    "@turf/distance": "latest",
    "@turf/helpers": "latest",
    "@turf/length": "latest",
    "amazon-quicksight-embedding-sdk": "latest",
    "angles": "latest",
    "aws-amplify": "^3.0.7",
    "axios": "0.18.1",
    "chart.js": "^2.9.3",
