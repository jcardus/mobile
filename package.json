{
  "name": "pinme",
  "description": "fleet management mobile",
  "version": "2.37.5",
  "license": "proprietary",
  "scripts": {
    "fontawesome": "npm config set \"@fortawesome:registry\" https://npm.fontawesome.com/ && npm config set \"//npm.fontawesome.com/:_authToken\" $FONTAWESOME_NPM_AUTH_TOKEN",
    "build": "vue-cli-service build",
    "report": "vue-cli-service build --report",
    "build:capacitor": "vue-cli-service build --mode capacitor --dest dist && npx cap copy",
    "build:android:nogartel": "cp dist/img/logos/nogartel.fleetmap.io.png dist/img/logos/localhost.png && ENVIRONMENT=android npx cap copy android",
    "deploy:android:localizalia": "bash mobile/Localizalia/build.sh android",
    "deploy:android:nogartel": "bash mobile/Nogartel/build.sh android",
    "deploy:android:manager": "bash mobile/Manager/build.sh android",
    "deploy:android": "bash mobile/build-android.sh",
    "deploy:android:fleetmap": "bash mobile/Manager/build-android.sh",
    "build:ios:localizalia": "mobile/Localizalia/build.sh ios",
    "build:ios:nogartel": "mobile/Nogartel/build.sh ios",
    "build:ios:manager": "mobile/Manager/build.sh ios",
    "build:ios:wuizy": "mobile/Wuizy/build.sh ios",
    "deploy:ios": "mobile/deploy-ios.sh",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "lint": "eslint --ext .js,.vue src",
    "build:stage": "vue-cli-service build --mode staging",
    "dev": "vue-cli-service serve",
    "dev:ios": "ENVIRONMENT=ios npx cap copy ios && npx cap open ios",
    "dev:android": "npx cap sync android && npx cap copy android",
    "serve:mobile": "npx cap serve",
    "new": "plop",
    "preview": "node build/index.js",
    "test:ci": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "@aws-amplify/auth": "3.4.34",
    "@aws-amplify/core": "3.8.24",
    "@capacitor-community/fcm": "latest",
    "@capacitor/android": "5.2.1",
    "@capacitor/app": "5.0.6",
    "@capacitor/browser": "5.0.6",
    "@capacitor/core": "5.2.1",
    "@capacitor/device": "5.0.6",
    "@capacitor/ios": "^5.2.1",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/push-notifications": "5.0.6",
    "@capawesome/capacitor-app-update": "^5.0.1",
    "@fortawesome/fontawesome-pro": "^6.5.1",
    "@mapbox/mapbox-gl-draw": "1.4.2",
    "@mapbox/mapbox-gl-geocoder": "5.0.1",
    "@sentry/integrations": "^7.60.0",
    "@sentry/vue": "7.60.0",
    "@turf/along": "latest",
    "@turf/area": "latest",
    "@turf/bbox": "latest",
    "@turf/bbox-polygon": "6.5.0",
    "@turf/bearing": "latest",
    "@turf/boolean-contains": "6.5.0",
    "@turf/distance": "latest",
    "@turf/helpers": "latest",
    "@turf/length": "latest",
    "angles": "latest",
    "axios": "1.4.0",
    "chart.js": "^3.3.2",
    "chartjs-adapter-moment": "^1.0.0",
    "chartjs-plugin-annotation": "^1.0.2",
    "el-table-infinite-scroll": "^1.0.10",
    "element-ui": "2.15.13",
    "fleetmap-partners": "1.0.126",
    "fleetmap-reports": "1.0.816",
    "framework7": "^5.7.14",
    "framework7-vue": "^5.7.14",
    "mapbox-gl": "latest",
    "mapbox-gl-controls": "latest",
    "moment": "^2.29.4",
    "nprogress": "latest",
    "odometer": "latest",
    "vue": "^2.6.14",
    "vue-i18n": "^8.22.2",
    "vue-moment": "^4.1.0",
    "vue-odometer": "latest",
    "vue-router": "3.0.2",
    "vue-static": "latest",
    "vue-timers": "latest",
    "vuejs-logger": "^1.5.5",
    "vuex": "^3.6.0",
    "vuex-persist": "latest"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/register": "7.0.0",
    "@capacitor/cli": "^5.2.1",
    "@sentry/webpack-plugin": "^1.20.0",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-pwa": "^4.5.9",
    "@vue/cli-plugin-unit-jest": "3.5.3",
    "@vue/cli-service": "^4.5.11",
    "@vue/runtime-dom": "^3.4.15",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.8.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "cordova-res": "^0.15.4",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2",
    "moment-locales-webpack-plugin": "^1.2.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.30.0",
    "sass-loader": "10.1.1",
    "typescript": "^4.4.4",
    "vue-template-compiler": "2.6.14",
    "webpack-version-file": "^0.1.6"
  }
}
